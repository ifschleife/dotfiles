Host *
    ControlMaster auto
    ControlPath ~/.ssh/socket-%r@%h:%p
    Compression yes
    AddressFamily inet

Host berry
    Hostname @BERRY@
    IdentityFile ~/.ssh/id_ecdsa
    user orim
    Port 33
    UserKnownHostsFile /dev/null
    StrictHostKeyChecking no
    TCPKeepAlive yes
    ForwardAgent yes
    LogLevel=quiet

Host mistress
    Hostname mistress
    IdentityFile ~/.ssh/id_ecdsa
    user orim
    TCPKeepAlive yes
    ForwardAgent yes

Host uberspace
    Hostname @UBERSPACE@
    IdentityFile ~/.ssh/id_rsa_uberspace
    user daremra
    TCPKeepAlive yes
    ForwardAgent yes

Host github
    Hostname github.com
    User git
    IdentityFile ~/.ssh/id_rsa_github

Host 192.168.{98,99}.*
    User drahier
    IdentityFile ~/.ssh/id_rsa_esigma
